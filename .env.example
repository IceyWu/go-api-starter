# Application Environment
APP_ENV=development

# Server Configuration
SERVER_HOST=localhost
SERVER_PORT=9527
SERVER_MODE=debug

# Database Configuration
DB_DRIVER=sqlite
DB_PATH=./data.db

# MySQL Configuration (if using MySQL)
# DB_DRIVER=mysql
# DB_HOST=localhost
# DB_PORT=3306
# DB_USER=root
# DB_PASSWORD=123456
# DB_NAME=go_api_starter

# Log Configuration
LOG_LEVEL=debug
LOG_FORMAT=console
LOG_OUTPUT=stdout

# Redis Configuration
REDIS_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_POOL_SIZE=10
REDIS_CLUSTER_MODE=false
REDIS_ENABLE_FALLBACK=true

# Mail Configuration
MAIL_ENABLED=true
MAIL_HOST=smtp.qq.com
MAIL_PORT=587
MAIL_USER=your-email@qq.com
MAIL_PASS=your-smtp-password
MAIL_FROM=
MAIL_USE_TLS=true
MAIL_MOCK_SEND=false

# OSS Configuration
ALICLOUD_OSS_ENDPOINT=your-bucket.oss-accelerate.aliyuncs.com
ALICLOUD_OSS_BUCKET=your-bucket
ALICLOUD_OSS_REGION=oss-cn-hangzhou
ALICLOUD_ACCESS_KEY_ID=your-access-key-id
ALICLOUD_ACCESS_KEY_SECRET=your-access-key-secret
ALICLOUD_OSS_UPLOAD_DIR=go_oss
OSS_DOMAIN=
OSS_CALLBACK_URL=
